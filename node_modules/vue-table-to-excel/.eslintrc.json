{
    "env": {
        "browser": true,
        "es6": true,
        "node":true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-extra-parens": "error", //禁止冗余的括号
        "no-extra-semi": "error" //禁用不必要的分号
    }
}